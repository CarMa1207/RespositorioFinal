<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.trabajofinal.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" text="EL JUEGO DE LA VIDA " />
   <Button fx:id="cargarPartida" contentDisplay="TEXT_ONLY" mnemonicParsing="false" onAction="#cargarPartidaClick" prefHeight="30.0" prefWidth="214.0" text="Cargar Partida " textFill="#b22727" />
   <Button fx:id="nuevaPartida" minHeight="17.0" minWidth="50.0" mnemonicParsing="false" onAction="#nuevaPartidaClick" prefHeight="32.0" prefWidth="210.0" text="Nueva Partida " />

</VBox>
